<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.another_flutterapp">

    <!-- Permissions required for network calls and biometric authentication -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.USE_BIOMETRIC" />
    <!-- Optional: Required for older Android versions (API levels 23-27) -->
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:label="another_flutterapp"
        android:icon="@mipmap/ic_launcher">

        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:launchMode="singleTop"
            android:taskAffinity=""
            android:theme="@style/LaunchTheme"
            android:configChanges="orientation|keyboardHidden|keyboard|screenSize|
                                   smallestScreenSize|locale|layoutDirection|
                                   fontScale|screenLayout|density|uiMode"
            android:hardwareAccelerated="true"
            android:windowSoftInputMode="adjustResize">

            <!-- Specifies an Android theme to apply as soon as the app starts -->
            <meta-data
                android:name="io.flutter.embedding.android.NormalTheme"
                android:resource="@style/NormalTheme" />

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Auto-generated by Flutter; do not remove -->
        <meta-data
            android:name="flutterEmbedding"
            android:value="2" />

        <!-- Placeholder for a Google Maps or other key.
             If this is your OpenAI key, consider placing it elsewhere
             (e.g., backend server or Dart code) to avoid exposing it. -->
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="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" />
    </application>

    <!-- Example query usage for certain intent-based actions -->
    <queries>
        <intent>
            <action android:name="android.intent.action.PROCESS_TEXT" />
            <data android:mimeType="text/plain" />
        </intent>
    </queries>
</manifest>



